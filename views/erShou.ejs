<html>
<%- include("./share/headErShou.ejs") %>
<body>
<div class="container border" id="app">
    <%- include("./share/nav.ejs") %>
    <div class="search-panel row pt-2 pb-2 border-bottom">
        <div class="input-group align-items-center">
            <input v-model="searchText" type="text" class="form-control fs-7"
                   placeholder="请输入关键字搜索" aria-label="菲律宾同城交易"/>
            <div @click="search" class="icofont-search-2 ms-2 pt-2 pb-2 text-center" style="width: 32px"></div>
        </div>
    </div>
    <div class="list-panel">
        <div class="list-item ">
            <div class="d-flex justify-content-between align-items-center">
                <i class="icofont-hotel-boy-alt text-warning fs-5"></i>
                <span class="fs-7 ms-1 text-secondary">大于和小胖</span>
            </div>
            <div>
                dsdfsdfsdfsdfsd
                sdf
                sdf
                sdf
                sdf
            </div>
            <div class="d-flex">
                <span class="text-info">价格：</span>
                <span >价面议</span>
            </div>
            <div class="d-flex">
                <span class="text-info">新旧程度：</span>
                <span >7成</span>
            </div>
            <div class="imgs-wrap">
                <div class="swiper mySwiper swiper-initialized swiper-horizontal swiper-pointer-events swiper-backface-hidden">
                    <div class="swiper-wrapper" id="swiper-wrapper-1015cbcca4dcae0c7" aria-live="polite">
                        <div class="swiper-slide swiper-slide-active" role="group" aria-label="1 / 9" style="width: 110px; margin-right: 30px;">Slide 1</div>
                        <div class="swiper-slide swiper-slide-next" role="group" aria-label="2 / 9" style="width: 110px; margin-right: 30px;">Slide 2</div>
                        <div class="swiper-slide" role="group" aria-label="3 / 9" style="width: 110px; margin-right: 30px;">Slide 3</div>
                        <div class="swiper-slide" role="group" aria-label="4 / 9" style="width: 110px; margin-right: 30px;">Slide 4</div>
                        <div class="swiper-slide" role="group" aria-label="5 / 9" style="width: 110px; margin-right: 30px;">Slide 5</div>
                        <div class="swiper-slide" role="group" aria-label="6 / 9" style="width: 110px; margin-right: 30px;">Slide 6</div>
                        <div class="swiper-slide" role="group" aria-label="7 / 9" style="width: 110px; margin-right: 30px;">Slide 7</div>
                        <div class="swiper-slide" role="group" aria-label="8 / 9" style="width: 110px; margin-right: 30px;">Slide 8</div>
                        <div class="swiper-slide" role="group" aria-label="9 / 9" style="width: 110px; margin-right: 30px;">Slide 9</div>
                    </div>
                    <div class="swiper-pagination swiper-pagination-clickable swiper-pagination-bullets swiper-pagination-horizontal"><span class="swiper-pagination-bullet swiper-pagination-bullet-active" tabindex="0" role="button" aria-label="Go to slide 1" aria-current="true"></span><span class="swiper-pagination-bullet" tabindex="0" role="button" aria-label="Go to slide 2"></span><span class="swiper-pagination-bullet" tabindex="0" role="button" aria-label="Go to slide 3"></span><span class="swiper-pagination-bullet" tabindex="0" role="button" aria-label="Go to slide 4"></span><span class="swiper-pagination-bullet" tabindex="0" role="button" aria-label="Go to slide 5"></span><span class="swiper-pagination-bullet" tabindex="0" role="button" aria-label="Go to slide 6"></span><span class="swiper-pagination-bullet" tabindex="0" role="button" aria-label="Go to slide 7"></span></div>
                    <span class="swiper-notification" aria-live="assertive" aria-atomic="true"></span></div>
            </div>
        </div>
    </div>
    <nav class="mt-2" aria-label="菲律宾同城交易">
        <ul class="pagination justify-content-center align-items-center">
            <li @click="goPage(pageNumber - 1)" class="page-item" :class="{disabled: pageNumber === 1}">
                <a class="page-link" href="javascript:void(0)" aria-label="Previous">
                    <span aria-hidden="true">&laquo;</span>
                </a>
            </li>
            <li v-if="pageNumber-1" @click="goPage(pageNumber-1)" class="page-item"><a class="page-link"
                                                                                       href="javascript:void(0)">{{pageNumber-1}}</a>
            </li>
            <li @click="goPage(pageNumber)" class="page-item active"><a class="page-link" href="javascript:void(0)">{{pageNumber}}</a>
            </li>
            <li v-if="pageNumber + 1 <= <%= totalPages; %>" @click="goPage(pageNumber + 1)" class="page-item"><a
                        class="page-link" href="javascript:void(0)">{{pageNumber+1}}</a>
            </li>
            <li @click="goPage(pageNumber + 1)" class="page-item"
                :class="{disabled: pageNumber === <%= totalPages; %>}">
                <a class="page-link" href="javascript:void(0);" aria-label="Next">
                    <span aria-hidden="true">&raquo;</span>
                </a>
            </li>
            <div class="ms-2">共<span class="text-success ms-1 me-1"><%= totalPages; %></span>页</div>
        </ul>
    </nav>
    <% if (user) { %>
        <img @click="goPost" class="position-fixed img-fluid" style="width: 50px;bottom: 40px; right: 20px;"
             src="/images/add.png"/>
    <% } else { %>
        <img @click="goLogin" class="position-fixed img-fluid" style="width: 50px;bottom: 40px; right: 20px;"
             src="/images/add.png"/>
        <!--<div @click="goLogin" class="position-fixed bg-primary text-light fs-6 rounded-circle text-center"
             style="width: 50px; height: 50px;line-height: 50px;bottom: 60px; right: 20px;"
        >登录
        </div>-->
    <% } %>
</div>
<script>
    var swiper = new Swiper(".mySwiper", {
        slidesPerView: 3,
        spaceBetween: 30,
        pagination: {
            el: ".swiper-pagination",
            clickable: true,
        },
    });
</script>
</body>
</html>